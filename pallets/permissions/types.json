{
  "SpacePermissions": "BTreeMap<SpacePermission, BuiltinRole>",

  "SpacePermissionsContext": {
    "space_id": "SpaceId",
    "is_space_owner": "bool",
    "is_space_follower": "bool",
    "space_perms": "SpacePermissions"
  },

  "SpacePermission": {
    "_enum": [
      "ManageRoles",
      "RepresentSpaceInternally",
      "RepresentSpaceExternally",
      "UpdateSpace",
      "BlockUsers",
      "CreateSubspaces",
      "UpdateOwnSubspaces",
      "UpdateAnySubspaces",
      "DeleteOwnSubspaces",
      "BlockSubspaces",
      "CreatePosts",
      "UpdateOwnPosts",
      "UpdateAnyPosts",
      "DeleteOwnPosts",
      "BlockPosts",
      "CreateComments",
      "UpdateOwnComments",
      "DeleteOwnComments",
      "BlockComments",
      "Upvote",
      "Downvote",
      "Share"
    ]
  },

  "BuiltinRole": {
    "_enum": [
      "None",
      "SpaceOwner",
      "Follower",
      "Everyone"
    ]
  }
}